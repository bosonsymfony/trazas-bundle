            "dev-master": "1.0.x-dev",
            "dev-b2.3":   "1.0.x-dev",
            "dev-b2.7":   "1.1.x-dev"
        }
    },
    "suggest": {
        "doctrine/mongodb-odm": "to use the functionalities availables for MongoDB ODM as SGBD"
    },
    "minimum-stability": "dev"

