{
    "name": "boson/trazas-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Paquete de trazas de Boson",
    "authors": [
        {
            "name": "Daniel Arturo Casals",
            "email": "dacasals@uci.cu"
        }
    ],
    "autoload": {
        "psr-0": {"UCI\\Boson\\TrazasBundle": ""}
    },
    "target-dir": "UCI/Boson/TrazasBundle",
    "require": {
        "symfony/symfony": "~2.7.0",
        "sensio/generator-bundle": "~2.3",
